{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es5",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "typeRoots": [
         "./src/@types",
         "./custom_typings",
         "./src/@types/express",
         "./@types",
         "./node_modules/@types"
      ]
   },
   
}